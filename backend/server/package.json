{
  "name": "@bfse/server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:schema": "graphql-codegen --config codegen.ts --only-schema"
  },
  "dependencies": {
    "@apollo/client": "^4.0.7",
    "@graphql-codegen/cli": "^6.0.1",
    "react": "^19.1.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@types/node": "^24.9.1",
    "@types/react": "^19.1.13",
    "dotenv": "^17.2.3",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "^19.1.1"
  }
}
